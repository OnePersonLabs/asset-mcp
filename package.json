{
  "name": "asset-mcp",
  "version": "0.1.0",
  "description": "AI agent asset generation framework enabling autonomous creation of 2D, 3D, and audio assets through a unified abstraction layer",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "ai",
    "agents",
    "assets",
    "3d",
    "2d",
    "audio",
    "abstraction",
    "autonomous",
    "workflow"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.1",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "eventsource": "^2.0.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/node": "^20.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}